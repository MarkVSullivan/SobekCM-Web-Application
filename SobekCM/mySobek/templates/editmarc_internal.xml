<?xml version="1.0" encoding="utf-8" ?> 

<!-- Begin the XML for this input template -->
<input_template
  xmlns="http://digital.uflib.ufl.edu/metadata/sobekcm_template/"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://digital.uflib.ufl.edu/metadata/sobekcm_template/
		http://digital.uflib.ufl.edu/metadata/sobekcm_template/sobekcm_template.xsd">

	<!-- Define the information about this input template -->
	<name>Internal Template</name>
	<notes>Complete template holds every field which could be editable online and viewable by internal users</notes>
	<dateCreated>August 20, 2009</dateCreated>
	<lastModified>August 20, 2009</lastModified>
	<creator>Mark V Sullivan</creator>
	<includeUserAsAuthor>false</includeUserAsAuthor>
	<uploads>FILE</uploads>
	
	<!-- This defines the inputs which are available for the user -->
	<inputs>
		<page>
			<name language="eng">Material Information</name>
      <panel>
        <name>Basic Information</name>
        <element type="Title" subtype="form" mandatory="true" acronym="Enter the title of your chapter, article, report, or other material here." />
        <element type="OtherTitle" subtype="form" mandatory="false" acronym="Enter the any additional titles for your material here." />
        <element type="Type" subtype="form" mandatory="true" acronym="Select the type which best categorizes this material." >
          <element_data>
            <options>Aerial, Archival, Artifact, Audio, Book, Map, Newspaper, Photograph, Serial, Video</options>
          </element_data>
        </element>
        <element type="Language" repeatable="true" acronym="Enter the language(s) of the material." >
        </element>
      </panel>
      <panel>
        <name language="eng">Name Authorities</name>
        <element type="Creator" subtype="form" repeatable="true" acronym="Enter each person or group which created this material. Personal names should be entered as [Family Name], [Given Name]." />
        <element type="Donor" />
      </panel>
      <panel>
        <name language="eng">Publication Details</name>
        <element type="Publisher" repeatable="true" subtype="complex" acronym="Enter the name(s) of the publisher(s) of the larger body of work.  If your work is currently unpublished, you may enter your name as the publisher or leave the field blank.  If you are adding administrative material (newsletters, handbooks, etc.) on behalf of a department within the university, enter the name of your department as the publisher." />
        <element type="Manufacturer" repeatable="true" subtype="complex" />
        <element type="Date" acronym="Enter the year in which your material was published.  If your material has not been published, enter the current year or leave the field blank." />
        <element type="DateCopyrighted" />
        <element type="Edition" />
      </panel>
    </page>
    <page>
      <name language="eng">Subjects and Notes</name>
      <panel>
        <name language="eng">Subjects</name>
        <element type="Subject" subtype="form" repeatable="true" />
		<element type="Genre" repeatable="true" />
        <element type="Temporal" subtype="complex" />
        <element type="Spatial" repeatable="true" />
        <element type="Coordinates" repeatable="true" />
        <element type="TargetAudience" />
      </panel>
      <panel>
        <name language="eng">Notes</name>
        <element type="Abstract" subtype="complex" repeatable="true" acronym="Enter your abstract here.  If your material does not have an abstract, you may include a summary of your document here."  />
        <element type="Note" subtype="complex" repeatable="true" acronym="Enter any additional information that is important for describing your material.  Include significant keywords or other data useful for finding your material in a citation-only search." />
      </panel>
    </page>
    <page>
      <name language="eng">Record Information</name>
      <panel>
        <name language="eng">Internal Record</name>
        <element type="Bib" mandatory="true" readonly="true" />
        <element type="VID" mandatory="true" readonly="true" />
        <element type="RecordStatus" />
        <element type="CreatorNotes" />
        <element type="RecordOrigin" />
        <element type="EncodingLevel" />
        <element type="Source" subtype="complex" />
        <element type="Holding" subtype="complex" />
        <element type="Tickler" />
        <element type="Rights" />
      </panel>
      <panel>
        <name language="eng">External Records</name>
        <element type="Identifier" subtype="complex" repeatable="true" />
		<element type="Classification" repeatable="true" />
        <element type="SerialHierarchy" />
        <element type="EAD" />
        <element type="Container" />
        <element type="OtherURL" />
        <element type="RelatedItem" repeatable="true" />
      </panel>
		<panel>
			<name language="eng">Downloads</name>
			<element type="Download" repeatable="true" />
		</panel>
    </page>
	</inputs>

</input_template>
<!-- End of input template XML -->